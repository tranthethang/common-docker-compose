# Docker Configuration
RESTART_POLICY=always

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=sonarqube  # Default database for SonarQube
POSTGRES_PORT=5432

# Adminer Configuration
ADMINER_PORT=8048

# Mailpit Configuration
MAILPIT_WEB_PORT=8025
MAILPIT_SMTP_PORT=1025

# MySQL Configuration
MYSQL_DATABASE=cloud
MYSQL_USER=uid
MYSQL_PASSWORD=pwd
MYSQL_ROOT_PASSWORD=secret
TZ=Asia/Ho_Chi_Minh
MYSQL_PORT=3306

# Memcached Configuration
MEMCACHED_PORT=11211

# MongoDB Configuration
MONGO_ROOT_USER=root
MONGO_ROOT_PASSWORD=example
MONGO_PORT=27017

# RabbitMQ Configuration
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_PORT=5672
RABBITMQ_WEB_PORT=15672

# Redis Configuration
REDIS_PASSWORD=changeme
REDIS_PORT=6379

# SonarQube Configuration
SONARQUBE_PORT=9090
# SonarQube uses the PostgreSQL database
# POSTGRES_USER and POSTGRES_PASSWORD are shared with PostgreSQL configuration

# Portainer Configuration
PORTAINER_PORT=9443

# MinIO Configuration
MINIO_API_PORT=9009
MINIO_CONSOLE_PORT=9001
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=password102
MINIO_DEFAULT_BUCKETS=my-bucket

# AI UI (AnythingLLM) Configuration
ANYTHINGLLM_PORT=3030
ANYTHINGLLM_STORAGE_DIR=/app/server/storage
# Base URL of the Ollama server (service name used when both in compose)
OLLAMA_BASE_URL=http://ollama:11434

# Ollama Configuration
# Public port mapped for the Ollama API
OLLAMA_PORT=11434
# Model to pull automatically at startup
OLLAMA_MODEL=gemma:7b